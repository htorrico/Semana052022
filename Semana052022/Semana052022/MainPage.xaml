<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Semana052022.MainPage">

    <StackLayout>
        <Button Text="Mi primer botón" BackgroundColor="Green"
                HorizontalOptions="CenterAndExpand" TextColor="White"
                ></Button>
        <DatePicker Date="09/25/2022" MaximumDate="09/27/2022" ></DatePicker>
        <DatePicker Date="09/27/2022" MinimumDate="09/27/2022" ></DatePicker>
        <Label Text="Mi Primer Label" FontSize="Large"  TextColor="White"
               BackgroundColor="Blue"/>
        <Entry Placeholder="Mi primer entry" MaxLength="20"></Entry>
        <Editor Placeholder="Mi primer editor" HeightRequest="100"></Editor>

        <DatePicker x:Name="dtpFechaNacimiento"></DatePicker>
        <TimePicker ></TimePicker>
        <Image Source="logo.png"></Image>
    </StackLayout>

</ContentPage>
